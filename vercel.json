{
  "redirects": [
    { "source": "/", "destination": "https://github.com/CitybuildCommunity/wormhole" }
  ],
  "rewrites": [
    {
      "source": "/:language/:query",
      "destination": "/api/index?q=:query&lang=:language"
    },
    { "source": "/:path*", "destination": "/api/index?q=:path*" }
  ]
}
